{
    "name": "parkball-tool",
    "displayName": "parkball-tool",
    "description": "simplify the process of modify folder and files in source directory",
    "version": "1.0.0",
    "publisher": "Vico",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createComponent"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createComponent",
                "title": "new",
                "category": "parkball"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "moment": "^2.22.2"
    }
}
